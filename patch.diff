diff --git a/src/lambdas/tickets/handler.py b/src/lambdas/tickets/handler.py
new file mode 100644
--- /dev/null
+++ b/src/lambdas/tickets/handler.py
@@
+import json
+from ...adapters.jira_client import JiraClient
+
+jira = JiraClient()
+
+def lambda_handler(event, context):
+    body = json.loads(event.get("body") or "{}")
+    summary = body.get("summary")
+    description = body.get("description", "")
+    tenant_id = body.get("tenant_id", "default")
+
+    if not summary:
+        return {"statusCode": 400, "body": "Missing summary"}
+
+    res = jira.create_ticket(summary, description, tenant_id=tenant_id)
+
+    return {"statusCode": 200, "body": json.dumps(res)}

