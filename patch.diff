+++ b/src/lambdas/pg_reservations/handler.py
@@
+import json
+from ...adapters.perfectgym_client import PerfectGymClient
+
+pg = PerfectGymClient()
+
+def lambda_handler(event, context):
+    body = json.loads(event.get("body") or "{}")
+
+    class_id = body.get("class_id")
+    member_id = body.get("member_id")
+    key = body.get("idempotency_key")
+    tenant = body.get("tenant_id", "default")
+
+    if not (class_id and member_id and key):
+        return {"statusCode": 400, "body": "Missing required fields"}
+
+    res = pg.reserve_class(member_id, class_id, key)
+    return {"statusCode": 200, "body": json.dumps(res)}

